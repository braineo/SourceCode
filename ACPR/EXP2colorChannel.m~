%% Hotelling T2 test for 15C2 combinations of pairs (Color 3*6 dimentional)
dMatrix = zeros(15,15);
pMatrix = zeros(15,15);
left = zeros(10,3*6);
right = zeros(10,3*6);
for subjectLeft = 1:15
    for subjectRight = subjectLeft:15
        for i = 1:10
            j = 1;
            for regioni = 1:6
                rangeL = (regioni-1)*10+1;
                rangeR = (regioni-1)*10+3;
                left(i,j:j+3) = EXP_INDV_REGION_NOANGLE_ms6{subjectLeft}.mInfo_tune{i}.weight(rangeL:rangeR);
                right(i,j:j+3) = EXP_INDV_REGION_NOANGLE_ms6{subjectRight}.mInfo_tune{i}.weight(rangeL:rangeR);
                j = j+3;
            end
        end
        X = [left;right];
        group = zeros(size(X,1),1);
        group(1:size(left,1)) = subjectLeft;
        group(1+size(left,1):end) = subjectRight;
        [d,p,stats] = manova1(X,group);